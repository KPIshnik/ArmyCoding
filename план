/todotilst/:id? todonote & priority & done 
    get -- get todolist  or lists
    post -- create todolist 
    put -- change todolist 
    delete -- delete todolist

/todotilst/permitted/:id
    get -- get todolist  or lists

/todotilst/share/:id?useremail
    get -- users shared with
    post -- share with user or users
    put -- share and unshare
    delete --  anshare with user/users
